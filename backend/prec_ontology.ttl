@prefix prec:  <http://bruy.at/prec#>       .
@prefix pvar:  <http://bruy.at/prec-trans#> .
@prefix pgo:   <http://ii.uwb.edu.pl/pgo#>  .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>       .

prec:UISuggestable a rdfs:Class ;
  rdfs:comment "Something that may be suggested"@en .

prec:UISuggestablePredicate a rdfs:Class ;
  rdfs:comment "The class of IRIs that may be suggested to be used in predicate position"@en .

prec:UISuggestableType a rdfs:Class ;
  rdfs:comment "The class of possibles types that may be usefull in PREC contexts"@en .


prec:UISuggestablePredicate rdfs:subClassOf prec:UISuggestable .
prec:UISuggestableType rdfs:subClassOf prec:UISuggestable .

prec:Edges a prec:UISuggestable ;
  rdfs:comment "All edges of the PG. Used as the subjet of a triple 'prec:Edges prec:templatedBy ...'" .

prec:templatedBy a prec:UISuggestable ;
  rdfs:comment "Used as the predicate to link all edges, all properties, an edge rule or a property rule to a template that describes how to represent it." .

prec:Relationships prec:sameAs prec:Edges .

prec:RdfStarUnique a prec:UISuggestable ;
  rdfs:comment "Edges are represented as a direct triple in the form << source edgeLabelIRI destination >>. Incurs information loss if the edge is not unique." .
prec:RdfStarOccurrence a prec:UISuggestable ;
  rdfs:comment "Edges are represented as an RDF star occurrence (see RDF-star documentation)" .

prec:RDFReification a prec:UISuggestable ;
  rdfs:comment "Edges are represented with the standard RDF reification format" .

pgo:Node a prec:UISuggestable ;
  rdfs:comment "Types of nodes in the PGO ontology and the PREC-0 representation" .
